{
	"name":         "@derhuerst/location",
	"description":  "Get your current location from the OS.",
	"version":      "1.0.0",
	"main": "./index.js",
	"files": [
		"index.js",
		"native.js"
	],
	"keywords":     ["geolocation", "location", "gps", "os x", "CoreLocation"],
	"author":       "Jannis R <mail@jannisr.de>",
	"homepage":     "https://github.com/derhuerst/location",
	"repository":   "git://github.com/derhuerst/location.git",
	"license":      "ISC",
	"engines":      {"node": ">=4"},
	"scripts": {
		"download":   "curl -sOL https://github.com/fulldecent/corelocationcli/releases/download/3.0.1/CoreLocationCLI.zip",
		"unzip":      "unzip -qq -n -d . CoreLocationCLI.zip",
		"cleanup":    "rm CoreLocationCLI.zip",
		"test":       "node test/index.js",
		"preinstall": "npm run download && npm run unzip && npm run cleanup",
		"prepublishOnly": "npm run preinstall && npm test"
	},
	"dependencies": {
		"wifi-triangulate": "^1.1.2"
	}
}
